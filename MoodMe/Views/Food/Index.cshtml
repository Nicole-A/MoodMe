@model List<MoodMe.Models.Food>
@{
    ViewData["Title"] = "Index";
}


<h1>Interesting...</h1>

<br />
<br />

<h2>What did you eat?</h2>

<br />


<form asp-controller="Food" asp-action="Submit" method="post">
    <select id="food">

        @foreach (var food in Model)
        {
            <option>@food.Name</option>
        }

    </select>


    <br />
    <br />
    <br />

    <input type="submit" value="Submit" />
</form>
